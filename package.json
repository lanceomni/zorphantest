{
  "name": "zorphan",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:stage": "npm run versionUpdate && ng build --output-path=dist/zorphanStage --configuration zorphanStage --aot",
    "build:stageSilent": "ng build --output-path=dist/zorphanStage --configuration zorphanStage --aot",
    "deploy:stage": "bucket=stage.zorphan.com dist_path=../dist/zorphanStage node ./scripts/s3Deploy.js",
    "build:prod": "npm run versionUpdate && ng build --output-path=dist/zorphanProd --configuration zorphanProd --aot",
    "build:prodSilent": "ng build --output-path=dist/zorphanProd --configuration zorphanProd --aot",
    "deploy:prod": "bucket=textbox.app dist_path=../dist/zorphanProd node ./scripts/s3Deploy.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "versionUpdate": "node ./scripts/versionUpdate.js"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@angular/service-worker": "~13.0.0",
    "@ctrl/ngx-emoji-mart": "^6.2.0",
    "@microsoft/signalr": "^6.0.1",
    "@ngx-loading-bar/core": "^6.0.1",
    "@ngx-loading-bar/http-client": "^6.0.1",
    "angular-resizable-element": "^5.0.0",
    "caret-pos": "^2.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "aws-sdk": "^2.1066.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
